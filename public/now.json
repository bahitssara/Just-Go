{
    "version": 2,
    "alias": "just-go-app",
    "name": "just-go",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "^/favicon.ico$",
          "dest": "/favicon.ico"
         },
         {
          "src": "^/manifest.json$",
          "dest": "/manifest.json"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ]
  }